---
pagination:
    data: contentful-photos
    size: 1
    alias: photoSet
    addAllPagesToCollections: true
permalink: "{{ photoSet.slug | slug }}/"
tags: "photoSet"
layout: layout.liquid

renderData:
    title: "{{ photoSet.title }}"

---

<div class="columns is-multiline">
    {%- for photo in photoSet.photos -%}
    <div class="column is-one-third ">
        <div class="box">
            <div class="image is-3by2">
                <img srcset="https:{{photo.fields.file.url}}?w=480&h=320&fm=webp&q=80&fit=fill&f=faces 480w,
                             https:{{photo.fields.file.url}}?w=800&h=533&fm=webp&q=80&fit=fill&f=faces 800w" sizes="(max-width: 600px) 480px,
                            800px" src="https:{{photo.fields.file.url}}?w=480&h=320&fit=fill&f=faces" alt="Photo from {{ photoSet.title }}" loading="lazy">
                <a href="https:{{photo.fields.file.url}}?q=100" class="is-overlay"><span class=" icon ">
                        <i class="fas fa-download is-large has-text-white"></i>
                    </span></a>
            </div>
        </div>
    </div>
    {%- endfor -%}
</div>
